<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="640.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lagasse.scheduler.controller.CustomerView">
   <children>
      <VBox prefHeight="640.0" prefWidth="1000.0">
         <children>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label text="Customers" />
               </children>
            </StackPane>
            <StackPane prefHeight="290.0" prefWidth="1000.0">
               <children>
                  <TableView prefHeight="255.0" prefWidth="1000.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#onExitBtn" text="Back to Main" />
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
